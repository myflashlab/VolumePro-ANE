<?xml version="1.0" encoding="utf-8"?>
<application xmlns="http://ns.adobe.com/air/application/25.0">
    <id>com.doitflash.exVolume</id>
    <versionNumber>2.0.1</versionNumber>
    <supportedProfiles>mobileDevice</supportedProfiles>
    <filename>exVolume</filename>
    <name>Volume Manager</name>
    <android>
        <manifestAdditions><![CDATA[<manifest android:installLocation="auto">

		<uses-permission android:name="android.permission.INTERNET" />

		<!--The new Permission thing on Android works ONLY if you are targetting Android SDK 23 or higher-->
		<uses-sdk android:targetSdkVersion="23"/>

		<application>

			<activity>
				<intent-filter>
					<action android:name="android.intent.action.MAIN" />
					<category android:name="android.intent.category.LAUNCHER" />
				</intent-filter>
				<intent-filter>
					<action android:name="android.intent.action.VIEW" />
					<category android:name="android.intent.category.BROWSABLE" />
					<category android:name="android.intent.category.DEFAULT" />
				</intent-filter>
			</activity>

		</application>
</manifest>]]></manifestAdditions>
    </android>
    <initialWindow>
        <title>exVolume</title>
        <content>ex.swf</content>
        <visible>true</visible>
        <fullScreen>true</fullScreen>
        <autoOrients>false</autoOrients>
        <aspectRatio>portrait</aspectRatio>
        <renderMode>gpu</renderMode>
        <!--<depthAndStencil>true</depthAndStencil>-->
        <!-- required for 3D -->
        <systemChrome>standard</systemChrome>
    </initialWindow>
